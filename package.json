    "@iconify-json/heroicons": "^1.2.2",
    "@iconify-json/ic": "^1.2.2",
    "@iconify-json/skill-icons": "^1.2.0",
    "@nuxt/eslint": "1.4.1",
    "@nuxt/test-utils": "^3.19.0",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/test-utils": "^2.4.6",
